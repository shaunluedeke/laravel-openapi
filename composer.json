{
    "name": "shaunluedeke/laravel-openapi",
    "description": "Generate OpenAPI Specification for Laravel Applications",
    "keywords": [
        "laravel",
        "openapi",
        "api",
        "documentation",
        "docs",
        "rest",
        "swagger"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Yuldashev",
            "email": "misterio92@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3|^8.4",
        "ext-json": "*",
        "doctrine/dbal": "^2.6|^3.0|4.3.1",
        "goldspecdigital/oooas": "^2.7.1",
        "laravel/framework": "5.8.*|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "phpdocumentor/reflection-docblock": "^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.3|^6.0|^7.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.5.13|^10.5|^11.5",
        "portavice/laravel-pint-config": "^v2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Vyuldashev\\LaravelOpenApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vyuldashev\\LaravelOpenApi\\Tests\\": "tests/",
            "Examples\\Petstore\\": "examples/petstore/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vyuldashev\\LaravelOpenApi\\OpenApiServiceProvider"
            ]
        }
    },
    "scripts": {
        "cs": [
            "pint --config vendor/portavice/laravel-pint-config/pint.json --test"
        ],
        "csfix": [
            "pint --config vendor/portavice/laravel-pint-config/pint.json"
        ],
        "test": [
            "phpunit"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
